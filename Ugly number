class Solution {
    public boolean isUgly(int n) {
        if(n<=0)return false;
  boolean prime[]= new boolean[n+1];
     for(int i=1;i<=n;i++)
         prime[i]=true;
        for(int i=2;i*i<=n;i++){
            if(prime[i]){
                for(int j=i*i;j<=n;j+=i)
                    prime[j]=false;
            }
        }
        for(int i=6;i<=n;i++){
            if(n%i==0 && prime[i]) return false;
        }
        return true;
    }
}
