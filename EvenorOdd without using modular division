import java.util.Scanner;


 class EvenorOddDemo
 {
   public static void main(String args[])
   { 
    Scanner sc=new Scanner(System.in);
    System.out.println("Enter a value");
    int n=sc.nextInt();
    if((n&1)==0)
    System.out.println(n+" is even");
    else
    System.out.println(n+" is odd");
   }
 }
